<%- include('partials/header'); %>

<%if (messages.error) { %>
  <%=messages.error%>
<%}%>  

<div class="container">
    <form id="subscribeForm" method="POST" action="/login">
        <div class="mb-3">
          <label for="exampleInputEmail1" class="form-label">Email address</label>
          <input type="email" class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp" name="username">
          <div id="emailHelp" class="form-text">We'll never share your email with anyone else.</div>
        </div>
        <div class="mb-3">
          <label for="exampleInputPassword1" class="form-label" >Password</label>
          <input type="password" class="form-control" id="exampleInputPassword1" name="password">
        </div>
        <div class="mb-3">
          <div class="g-recaptcha" data-sitekey="6LeSqFsdAAAAAFmJEmLVBTjYSI15EnTbFUP25_zr"></div>
        </div>
        <button id="submit" type="submit" value="Submit" class="btn btn-primary">Submit</button>
    </form>
</div>

</body>
</html>